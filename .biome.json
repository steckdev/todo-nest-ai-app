{
  "linter": {
    "rules": {
      "suspicious": {
        "noDebugger": "error",
        "noConsoleLog": "warn"
      },
      "style": {
        "useConst": {
          "level": "warn",
          "fix": "safe"
        },
        "useTemplate": {
          "level": "warn",
          "fix": "safe"
        },
        "noShoutyConstants": "warn",
        "useNamingConvention": {
          "level": "error",
          "options": {
            "strictCase": false
          }
        }
      },
      "correctness": {
        "noUnusedVariables": {
          "level": "error",
          "fix": "none"
        }
      }
    }
  }
}
